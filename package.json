{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "format": "prettier --write \"./**/*.{ts,tsx}\" && npx sort-package-json",
    "postbuild": "next-sitemap --config custom-next-sitemap.js",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start-storybook": "npx http-server ./storybook-static",
    "migrate:db:push": "prisma db push",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:dev:create": "yarn prisma migrate dev --create-only",
    "migrate:deploy": "yarn prisma migrate deploy",
    "migrate:reset": "yarn prisma migrate reset",
    "migrate:resolve": "yarn prisma migrate resolve",
    "prisma:generate": "yarn prisma generate",
    "prisma:generate:watch": "yarn prisma generate --watch",
    "prisma:seed": "yarn prisma db seed",
    "prisma:studio": "yarn prisma studio",
    "prisma:run": "yarn prisma:generate && yarn migrate:db:push && yarn prisma:seed"
  },
  "dependencies": {
    "@prisma/client": "3.2.1",
    "@stitches/react": "1.2.5",
    "@trpc/client": "^9.9.1",
    "@trpc/next": "^9.9.1",
    "@trpc/react": "^9.9.1",
    "@trpc/server": "^9.9.1",
    "formik": "^2.2.9",
    "next": "11.1.2",
    "next-i18next": "^8.9.0",
    "next-pwa": "^5.3.2",
    "next-seo": "^4.28.1",
    "overmind": "^28.0.1",
    "overmind-react": "29.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loading-skeleton": "^2.2.0",
    "react-spinners": "^0.11.0",
    "react-toast-notifications": "^2.5.1",
    "superjson": "^1.7.5",
    "yup": "^0.32.11",
    "zod": "^3.9.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.30",
    "@types/react-toast-notifications": "^2.4.1",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "next-sitemap": "^1.6.184",
    "prettier": "^2.4.1",
    "prisma": "^3.2.1",
    "react-query": "^3.26.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2"
  }
}
